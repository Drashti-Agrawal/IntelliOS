{
  "name": "intelliosui",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^13.5.0",
    "bootstrap": "^5.3.5",
    "electron-is-dev": "^2.0.0",
    "lucide-react": "^0.541.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-router-dom": "^7.5.0",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "start-prod": "electron .",
    "electron": "cross-env ELECTRON_START_URL=http://localhost:3000 electron .",
    "start-electron": "npm run build && npm run electron",
    "build": "npm run react-build && electron-builder && react-scripts build",
    "dist": "npm run react-build && electron-builder --publish=never",
    "pack": "electron-builder --dir"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "autoprefixer": "^10.4.21",
    "cross-env": "^10.0.0",
    "electron": "^35.7.5",
    "electron-builder": "^23.6.0",
    "postcss": "^8.5.6",
    "react-scripts": "^5.0.1",
    "tailwindcss": "^3.4.17"
  },
  "build": {
    "appId": "com.mycompany.myapp-installer",
    "productName": "MyApp ML Monitoring Installer",
    "directories": {
      "output": "dist"
    },
    "files": [
      "build/**/*",
      "public/main.js",
      "public/preload.js",
      "node_modules/**/*"
    ],
    "win": {
      "target": "nsis",
      "icon": "build/icon.ico"
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true
    }
  }
}
